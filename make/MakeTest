all: sparse_test

sparse_test: main_test.o mystring.o rational_number.o vector.o matrix.o my_exeptions.o
				g++ main_test.o mystring.o rational_number.o vector.o matrix.o my_exeptions.o -o sparse_test

main_test.o: ../main_test.cpp
				g++ -c ../main_test.cpp

mystring.o: ../mystring.cpp
				g++ -c ../mystring.cpp

rational_number.o: ../rational_number.cpp
				g++ -c ../rational_number.cpp

vector.o: ../vector.cpp
				g++ -c ../vector.cpp

matrix.o: ../matrix.cpp
				g++ -c ../matrix.cpp

my_exeptions.o: ../my_exeptions.cpp
				g++ -c ../my_exeptions.cpp

clean:
	rm -rf ./sparse_test *.o sparse_test
